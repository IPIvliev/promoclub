                <% @promoters.each do |promoter| %>
                <!-- items -->
                <div class="col-sm-6 col-md-4"><!-- item -->
                  <div class="item-box">
                    <figure>
                      <a class="item-hover" href="<%= user_path(promoter) %>">
                        <span class="overlay color2"></span>
                        <span class="inner">
                          <span class="block fa fa-plus fsize20"></span>
                          <strong>ПОДРОБНАЯ</strong> ИНФОРМАЦИЯ
                        </span>
                      </a>
                      <%= image_tag promoter.avatar.thumb.url, :class => "img-responsive", :alt => "Промоутер: #{promoter.name} #{promoter.surname}", :title => "Промоутер: #{promoter.name} #{promoter.surname}" %>
                    </figure>
                    <div class="item-box-desc">
                      <h4><%= promoter.name_with_rate %></h4>
                      <small class="styleColor"><a href="/users?utf8=%E2%9C%93&q%5Bgender_eq%5D=&q%5Bage_lteq%5D=&q%5Bage_gteq%5D=&q%5Bcountry_id_eq%5D=<%= promoter.country.id %>&q%5Bstate_id_eq%5D=<%= promoter.state.id %>&q%5Bcity_id_eq%5D=<%= promoter.city.id %>&commit=%D0%9D%D0%B0%D0%B9%D1%82%D0%B8"><%= promoter.city.name %> </a></small>
                      <strong><%= calculate_age(promoter.birth) %> <%= Russian.pluralize(calculate_age(promoter.birth), "год", "года", "лет") %></strong>

                      <span class="fsize12 pull-right">
                        <%= "<i class='fa fa-medkit styleSecondColor' title='Имеется медицинская книжка'></i>".html_safe if promoter.med %>
                        <%= "<i class='fa fa-truck styleSecondColor' title='Имеется автомобиль'></i>".html_safe if promoter.car %>
                      </span>
                    </div>

                    
                  </div>
                </div>

                <% end %>